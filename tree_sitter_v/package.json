{
  "name": "tree-sitter-v",
  "version": "0.0.4-beta.1",
  "main": "bindings/node",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlang/v-analyzer.git"
  },
  "scripts": {
    "test": "tree-sitter test",
    "generate": "tree-sitter generate && v run generate_types.vsh",
    "parse": "tree-sitter parse",
    "parseg": "tree-sitter parse --debug-graph",
    "format": "prettier --write \"**/*.cjs\""
  },
  "dependencies": {
    "nan": "^2.18.0"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "tree-sitter-cli": "^0.21.0"
  },
  "tree-sitter": [
    {
      "scope": "source.v",
      "file-types": [
        "v",
        "vsh",
        "v.mod"
      ]
    }
  ]
}
